# Migration configuration - match your SAM deployment

environment = "prod"
aws_region  = "ca-central-1"

# What did SAM create? (check your SAM template parameters)
sam_created_kms              = true   # CreateKMSKey=true in SAM?
sam_created_config           = false  # CreateConfigService=true in SAM?
sam_created_encryption_rule  = true   # CreateEncryptionConfigRule=true in SAM?
sam_created_retention_rule   = true   # CreateRetentionConfigRule=true in SAM?
sam_created_eventbridge      = true   # CreateEventBridgeRules=true in SAM?
sam_created_dashboard        = true   # CreateMonitoringDashboard=true in SAM?

# If SAM used existing KMS key (CreateKMSKey=false)
# existing_kms_key_arn = "arn:aws:kms:..."

# Lambda settings (should match SAM deployment)
default_retention_days = 90
owner                  = "DevOps"
