{
  "LogGuardianFunction": {
    "KMS_KEY_ALIAS": "alias/logguardian-logs",
    "DEFAULT_RETENTION_DAYS": "365", 
    "ENVIRONMENT": "dev",
    "LOG_LEVEL": "DEBUG",
    "DRY_RUN": "true",
    "BATCH_LIMIT": "10",
    "BATCH_RESOURCE_DELAY_MS": "25",
    "BATCH_GROUP_DELAY_MS": "100"
  },
  "Parameters": {
    "LambdaTimeout": "30",
    "LambdaMemorySize": "256"
  }
}
